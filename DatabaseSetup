Create Table Supplier(
SupplierID Number(11,0) Not Null,
SupplierName VARCHAR2(30),
DateSupplied Date,
EmployeeID Number(11,0),
Constraint Supplier_PK Primary Key (SupplierID),
Constraint Supplier_FK Foreign Key (EmployeeID) References
Employee(EmployeeID));

