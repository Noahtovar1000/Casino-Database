Create table Employee(
EmployeeID Number(11,0) Not Null,
EmployeeFirstName VARCHAR2(20) ,
EmployeeLastName VARCHAR2(30) ,
DateHired Date,
Constraint Employee_PK Primary Key (EmployeeID));

